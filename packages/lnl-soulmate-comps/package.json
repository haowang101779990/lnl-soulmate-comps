{
  "name": "@lnl-soulmate/lnl-soulmate-comps",
  "version": "1.0.0",
  "description": "lnl soulmate comp lib",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./asset/*": "./dist/asset/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "release": "standard-version",
    "prettier": "prettier --write ./src"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haowang101779990/lnl-soulmate-comps"
  },
  "author": "hao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haowang101779990/lnl-soulmate-comps/issues"
  },
  "homepage": "https://github.com/haowang101779990/lnl-soulmate-comps#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@esotericsoftware/spine-pixi-v8": "~4.2.0",
    "@pixi/react": "^8.0.0-beta.25",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-links": "^9.0.18",
    "@storybook/addon-onboarding": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.23",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^9.32.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "9.0.18",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pixi.js": "^8.11.0",
    "prettier": "3.6.2",
    "react": "^19",
    "react-dom": "^19",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "storybook": "^9.0.18",
    "styled-components": "^6.1.19",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@esotericsoftware/spine-pixi-v8": "~4.2.0",
    "@pixi/react": "^8.0.0-beta.25",
    "pixi.js": "^8.11.0",
    "react": ">=19",
    "react-dom": ">=19",
    "styled-components": "^6.1.19"
  },
  "dependencies": {
    "@rollup/plugin-url": "^8.0.2"
  }
